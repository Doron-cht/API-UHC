+package fr.doron.uhc.plugin.game;
+
+public class Episode {
+
+    GameManager gameManager = new GameManager();
+
+    public enum Phase {
+        DAY,
+        NIGHT
+    }
+
+    private int number;       // numéro de l'épisode
+    private Phase phase;      // jour ou nuit
+    private int duration;     // durée totale de l'épisode en secondes
+    private int elapsed;      // temps écoulé dans la phase actuelle
+
+    public Episode(int number, int duration) {
+        this.number = number;
+        this.duration = duration;
+        this.phase = Phase.DAY; // toujours commencer par le jour
+        this.elapsed = 0;
+    }
+
+    public int getNumber() {
+        return number;
+    }
+
+    public Phase getPhase() {
+        return phase;
+    }
+
+    public void nextPhase() {
+        if (phase == Phase.DAY) {
+            phase = Phase.NIGHT;
+        } else {
+            phase = Phase.DAY;
+            number++; // nouvel épisode après la nuit
+        }
+        elapsed = 0;
+        gameManager.broadcastEpisodeMessage();
+
+    }
+
+    public void tick() {
+        elapsed++;
+    }
+
+    public int getTimeLeft() {
+        return duration - elapsed;
+    }
+
+    public boolean isPhaseOver() {
+        return elapsed >= duration;
+    }
+}
