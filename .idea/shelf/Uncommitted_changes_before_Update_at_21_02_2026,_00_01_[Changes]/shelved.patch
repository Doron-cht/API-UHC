+package fr.doron.dcuhc.roles.Tools;
+
+import fr.doron.dcuhc.roles.DCRoles;
+import net.md_5.bungee.api.chat.ClickEvent;
+import net.md_5.bungee.api.chat.ComponentBuilder;
+import net.md_5.bungee.api.chat.HoverEvent;
+import net.md_5.bungee.api.chat.TextComponent;
+import org.bukkit.entity.Player;
+
+public class DCRolePreview {
+
+    public static void sendAllRoles(Player player) {
+
+        player.sendMessage("§8====================================");
+        player.sendMessage("§6§lDC UHC - Liste des rôles");
+        player.sendMessage("§8====================================");
+
+        for (DCRoles dcRole : DCRoles.values()) {
+
+            TextComponent roleComponent = new TextComponent(" §e• " + dcRole.getDisplayName());
+
+            // Click → exécute la commande
+            roleComponent.setClickEvent(new ClickEvent(
+                    ClickEvent.Action.RUN_COMMAND,
+                    "/dc roles " + dcRole.name()
+            ));
+
+            // Hover → petit texte explicatif
+            roleComponent.setHoverEvent(new HoverEvent(
+                    HoverEvent.Action.SHOW_TEXT,
+                    new ComponentBuilder("§7Voir la description de §e" + dcRole.getDisplayName())
+                            .create()
+            ));
+
+            player.spigot().sendMessage(roleComponent);
+        }
+
+        player.sendMessage("§8====================================");
+    }
+}
