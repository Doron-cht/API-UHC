+import net.md_5.bungee.api.chat.ComponentBuilder;
+import net.md_5.bungee.api.chat.HoverEvent;
+import net.md_5.bungee.api.chat.TextComponent;
+
+import java.util.HashMap;
+import java.util.Map;
+import java.util.UUID;
+
+public class openStartInventoryGUI implements Listener {
+
+    private static final int INVENTORY_SIZE = 36;
+    private static final String INVENTORY_TITLE = ChatColor.DARK_GREEN + "Start Inventory";
+
+    // Temporary storage of player's inventory for next step
+    private static final Map<UUID, ItemStack[]> tempInventories = new HashMap<>();
+    // Temporary storage of player's original inventory before opening GUI
+    private static final Map<UUID, ItemStack[]> originalInventories = new HashMap<>();
+
+    // Open the GUI for the player to configure start inventory
+    public static void open(Player player) {
+        // Save original inventory before opening GUI
+        ItemStack[] originalContents = player.getInventory().getContents();
+        ItemStack[] originalArmor = player.getInventory().getArmorContents();
+        ItemStack[] combinedOriginal = new ItemStack[originalContents.length + originalArmor.length];
+        System.arraycopy(originalContents, 0, combinedOriginal, 0, originalContents.length);
+        System.arraycopy(originalArmor, 0, combinedOriginal, originalContents.length, originalArmor.length);
+        originalInventories.put(player.getUniqueId(), combinedOriginal);
+
+        // Clear player's inventory so they can freely choose items
+        player.getInventory().clear();
+
+        Inventory inv = Bukkit.createInventory(null, INVENTORY_SIZE, INVENTORY_TITLE);
+        // The player will be able to use the inventory like in creative mode, placing any items freely
+        player.openInventory(inv);
+    }
+
+    // Handle inventory clicks to prevent taking items out (optional, depends on desired behavior)
+    @EventHandler
+    public void onInventoryClick(InventoryClickEvent event) {
+        if (event.getView().getTitle().equals(INVENTORY_TITLE)) {
+            // Prevent shift-click or placing items outside the inventory
+            if (event.getRawSlot() >= INVENTORY_SIZE || event.getRawSlot() < 0) {
+                event.setCancelled(true);
+                return;
+            }
+        }
+    }
+
+    // When player closes the inventory, send the clickable message with three buttons
+    @EventHandler
+    public void onInventoryClose(InventoryCloseEvent event) {
+        if (!(event.getPlayer() instanceof Player)) return;
+        Player player = (Player) event.getPlayer();
+
+        if (!event.getView().getTitle().equals(INVENTORY_TITLE)) return;
+
+        Inventory inv = event.getInventory();
+
+        // Save the inventory contents temporarily
+        ItemStack[] contents = new ItemStack[INVENTORY_SIZE];
+        for (int i = 0; i < INVENTORY_SIZE; i++) {
+            ItemStack item = inv.getItem(i);
+            if (item != null) {
+                contents[i] = item.clone();
+            } else {
+                contents[i] = null;
+            }
+        }
+        tempInventories.put(player.getUniqueId(), contents);
+
+        // Create clickable message with three buttons: check (save), anvil (enchant), cross (cancel)
+        TextComponent message = new TextComponent(ChatColor.GREEN + "Choisissez une option ");
+
+        // Check button
+        TextComponent checkButton = new TextComponent("[\u2713] "); // check mark
+        checkButton.setColor(net.md_5.bungee.api.ChatColor.GREEN);
+        checkButton.setHoverEvent(new HoverEvent(HoverEvent.Action.SHOW_TEXT, new ComponentBuilder("Sauvegarder").create()));
+        checkButton.setClickEvent(new ClickEvent(ClickEvent.Action.RUN_COMMAND, "/startinventory save"));
+
+        // Anvil button
+        TextComponent anvilButton = new TextComponent("[\u2692] "); // anvil symbol
+        anvilButton.setColor(net.md_5.bungee.api.ChatColor.GOLD);
+        anvilButton.setHoverEvent(new HoverEvent(HoverEvent.Action.SHOW_TEXT, new ComponentBuilder("Enchanter").create()));
+        anvilButton.setClickEvent(new ClickEvent(ClickEvent.Action.RUN_COMMAND, "/startinventory enchant"));
+
+        // Cross button
+        TextComponent crossButton = new TextComponent("[\u2717]"); // cross mark
+        crossButton.setColor(net.md_5.bungee.api.ChatColor.RED);
+        crossButton.setHoverEvent(new HoverEvent(HoverEvent.Action.SHOW_TEXT, new ComponentBuilder("Annuler").create()));
+        crossButton.setClickEvent(new ClickEvent(ClickEvent.Action.RUN_COMMAND, "/startinventory cancel"));
+
+        message.addExtra(checkButton);
+        message.addExtra(anvilButton);
+        message.addExtra(crossButton);
+
+        player.spigot().sendMessage(message);
+    }
+
+    // Method to get the temporarily saved inventory for a player
+    public static ItemStack[] getTempInventory(Player player) {
+        return tempInventories.get(player.getUniqueId());
+    }
+
+    // Method to clear the temporary inventory when done or cancelled
+    public static void clearTempInventory(Player player) {
+        tempInventories.remove(player.getUniqueId());
+    }
+
+    // Handle the /startinventory save command
+    public static void handleSave(Player player) {
+        // Save the temp inventory as needed (implement your logic here, e.g., save to config/database)
+        // For now, just clear temp inventory and restore original inventory
+        clearTempInventory(player);
+        restoreOriginalInventory(player);
+        player.sendMessage(ChatColor.GREEN + "Inventaire de départ sauvegardé et votre inventaire original restauré.");
+    }
+
+    // Handle the /startinventory cancel command
+    public static void handleCancel(Player player) {
+        clearTempInventory(player);
+        restoreOriginalInventory(player);
+        player.sendMessage(ChatColor.RED + "Configuration de l'inventaire de départ annulée. Votre inventaire a été restauré.");
+    }
+
+    // Handle the /startinventory enchant command
+    public static void handleEnchant(Player player) {
+        ItemStack item = player.getInventory().getItemInHand();
+        if (item == null || item.getType() == Material.AIR) {
+            player.sendMessage(ChatColor.RED + "Vous devez tenir un objet pour l'enchanter.");
+            return;
+        }
+        // Placeholder: open an enchantment GUI or console
+        player.sendMessage(ChatColor.GOLD + "Fonctionnalité d'enchantement à venir ! (placeholder)");
+    }
+
+    // Method to restore the original inventory after saving
+    public static void restoreOriginalInventory(Player player) {
+        ItemStack[] original = originalInventories.get(player.getUniqueId());
+        if (original != null) {
+            int contentsLength = player.getInventory().getSize();
+            int armorLength = original.length - contentsLength;
+            ItemStack[] contents = new ItemStack[contentsLength];
+            ItemStack[] armor = new ItemStack[armorLength];
+            System.arraycopy(original, 0, contents, 0, contentsLength);
+            System.arraycopy(original, contentsLength, armor, 0, armorLength);
+            player.getInventory().setContents(contents);
+            player.getInventory().setArmorContents(armor);
+            originalInventories.remove(player.getUniqueId());
+        }
+    }
+    public static Map<UUID, ItemStack[]> getStartInventory() {
+        Map<UUID, ItemStack[]> copy = new HashMap<>();
+        for (Map.Entry<UUID, ItemStack[]> entry : tempInventories.entrySet()) {
+            ItemStack[] original = entry.getValue();
+            ItemStack[] clone = original != null ? original.clone() : null;
+            copy.put(entry.getKey(), clone);
+        }
+        return copy;
+    }
+}
