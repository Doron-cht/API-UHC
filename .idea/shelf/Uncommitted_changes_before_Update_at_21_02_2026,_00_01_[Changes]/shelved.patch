+package fr.doron.uhc.plugin.listeners;
+
+import fr.doron.uhc.api.ResurrectAbility;
+import fr.doron.uhc.plugin.UHCAPI;
+import fr.doron.uhc.plugin.game.GameManager;
+import net.md_5.bungee.api.chat.ClickEvent;
+import net.md_5.bungee.api.chat.TextComponent;
+import org.bukkit.Bukkit;
+import org.bukkit.entity.Player;
+import org.bukkit.event.EventHandler;
+import org.bukkit.event.Listener;
+import org.bukkit.event.entity.PlayerDeathEvent;
+
+import java.util.List;
+
+public class PlayerDeath implements Listener {
+
+    private final GameManager gameManager;
+
+    private static final List<ResurrectAbility> resurrectAbilities = UHCAPI.getInstance().getResurrectAbilities();
+
+    public PlayerDeath() {
+        this.gameManager = UHCAPI.getInstance().getGameManager();
+    }
+
+    @EventHandler
+    public void onDeath(PlayerDeathEvent e) {
+        Player player = e.getEntity();
+        if (player == null) return;
+        for (ResurrectAbility ability : resurrectAbilities) {
+            for (Player helper : UHCAPI.getInstance().getPlayersForTheGame()) {
+                if (ability.canResurrect(player, helper)) {
+                    // Envoyer un message cliquable pour le joueur helper
+                    TextComponent msg = new TextComponent("§aCliquez ici pour ressusciter " + player.getName());
+                    msg.setClickEvent(new ClickEvent(ClickEvent.Action.RUN_COMMAND, "/resurrect " + player.getName()));
+                    helper.spigot().sendMessage(msg);
+                }
+            }
+        }
+        // Retirer le joueur de la liste des joueurs de la partie
+        UHCAPI.getInstance().removePlayer(player);
+
+        // Vérifier la victoire après un tick
+        Bukkit.getScheduler().runTaskLater(UHCAPI.getInstance(), gameManager::checkVictory, 1L);
+    }
+}
