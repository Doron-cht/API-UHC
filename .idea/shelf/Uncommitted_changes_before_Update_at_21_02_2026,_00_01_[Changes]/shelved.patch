+package fr.doron.dcuhc.Camp;
+
+import fr.doron.uhc.api.GameContext;
+import fr.doron.uhc.api.Role;
+import org.bukkit.ChatColor;
+import org.bukkit.entity.Player;
+
+public enum Camp implements fr.doron.uhc.api.Camp {
+
+    HERO("Héros", ChatColor.GREEN),
+    MAFIA("Mafia", ChatColor.YELLOW),
+    SUICIDESQUAD("Suicide Squad", ChatColor.DARK_PURPLE),
+    VILLAIN("Vilain", ChatColor.RED),
+    SOLO("Solo", ChatColor.GOLD);
+
+    private final String name;
+    private final ChatColor color;
+
+    Camp(String name, ChatColor color) {
+        this.name = name;
+        this.color = color;
+    }
+
+    @Override
+    public String getName() {
+        return name;
+    }
+
+    public ChatColor getColor() {
+        return color;
+    }
+
+    public boolean isHero(){
+        return this == HERO;
+    }
+
+    public boolean isMafia(){
+        return this == MAFIA;
+    }
+
+    public boolean isVillain(){
+        return this == VILLAIN;
+    }
+
+    public boolean isSuicideSquad(){
+        return this == SUICIDESQUAD;
+    }
+
+
+    public boolean isSolo() {
+        return this == SOLO;
+    }
+
+    @Override
+    public boolean hasWon(GameContext context) {
+        // Vérifier si le camp est SOLO
+        if (isSolo()) {
+            // Si un seul joueur est encore en vie dans la partie
+            if (context.getAlivePlayers().size() == 1) {
+                Player lastPlayer = context.getAlivePlayers().iterator().next();
+                Role role = context.getRole(lastPlayer);
+                // Vérifier si le camp du dernier joueur correspond à ce camp (SOLO)
+                return role.getCamp() == this;
+            }
+            // S'il y a plus d'un joueur en vie, aucun solo n'a encore gagné
+            return false;
+        } else {
+            // Pour les camps normaux, vérifier s'il reste des joueurs vivants n'appartenant pas à ce camp
+            for (Player p : context.getAlivePlayers()) {
+                Role r = context.getRole(p);
+                // S'il existe au moins un joueur vivant dont le camp est différent, ce camp n'a pas gagné
+                if (r.getCamp() != this) {
+                    return false;
+                }
+            }
+            // S'il ne reste aucun ennemi en vie, ce camp a gagné
+            return true;
+        }
+    }
+}
