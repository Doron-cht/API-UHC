+                UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+                break;
+
+            case "§e• §f-15":
+                if (UHCAPI.getInstance().getGameManager().getEpisodeDuration() - 15 <= 20) {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(20);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+                } else {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(UHCAPI.gameManager.getEpisodeDuration() - 15);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+                }
+                break;
+            case "§e• §f-30":
+                if (UHCAPI.getInstance().getGameManager().getEpisodeDuration() - 30 <= 20) {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(20);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+
+                } else {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(UHCAPI.gameManager.getEpisodeDuration() - 30);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+                }
+                break;
+
+            case "§e• §f-60":
+                if (UHCAPI.getInstance().getGameManager().getEpisodeDuration() - 60 <= 20) {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(20);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+
+                } else {
+                    UHCAPI.getInstance().getGameManager().setEpisodeDuration(UHCAPI.gameManager.getEpisodeDuration() - 60);
+                    UHCAPI.getInstance().getGUIManager().open(player, HostEpisodeGUI.class);
+                }
+                break;
+
+            case "§e• §fRetour":
+                player.sendMessage("§e• §7Retour");
+                player.closeInventory();
+                UHCAPI.getInstance().getGUIManager().open(player, HostTimerGUI.class);
+                break;
+
+            default: break;
+        }
+    }
+}
