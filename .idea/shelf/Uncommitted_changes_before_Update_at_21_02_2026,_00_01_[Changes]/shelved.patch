+
+    private ItemStack EpisodeItem() {
+        ItemStack item = new ItemStack(Material.BOOK);
+        ItemMeta meta = item.getItemMeta();
+        int minutes = UHCAPI.getInstance().getGameManager().EpisodeDurationMinutes();
+        int seconds = UHCAPI.getInstance().getGameManager().EpisodeDurationSeconds();
+        meta.setDisplayName("§e"+puce()+" §fDurée d'un épisode");
+        meta.setLore(Collections.singletonList("§7" + guillemet() + " §eDurée d'un épisode : §f" + minutes + " minutes " + seconds + " secondes"));
